<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.forbeautysake">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fbs_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fbs_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AdminAddCategory"></activity>
        <activity android:name=".ReviewByCategory" />
        <activity android:name=".EditReview" />
        <activity android:name=".NewReview" />
        <activity android:name=".Dashboard" />
        <activity android:name=".Register" />
        <activity android:name=".AdminDashboard" />
        <activity android:name=".AdminAddPromo" />
        <activity android:name=".AdminEditPromo" />
        <activity
            android:name=".Login"
            android:label=""
            android:noHistory="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>